-- Question 1
SELECT u.name as city
FROM hw5.world x, x.mondial.country y, y.province z,
                   CASE  WHEN z.city is missing THEN []
                   WHEN is_array(z.city) THEN z.city
                   ELSE [z.city] END u
WHERE  y.name='Peru'
ORDER BY u.name;

